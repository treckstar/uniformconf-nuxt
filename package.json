    "postcss": "^8.4.14",
    "tailwindcss": "^3.0.24"
  },
  "dependencies": {
    "@uniformdev/canvas": "16.0.1-nuxt.159",
    "@uniformdev/canvas-vue": "16.0.1-nuxt.159",
    "@uniformdev/context": "16.0.1-nuxt.159",
    "@uniformdev/context-edge": "16.0.1-nuxt.159",
    "@uniformdev/context-edge-netlify": "16.0.1-nuxt.159",
